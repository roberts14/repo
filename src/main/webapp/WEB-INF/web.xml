   </servlet-mapping>

   <servlet-mapping>
      <servlet-name>Controller</servlet-name>
      <url-pattern>/control</url-pattern>
   </servlet-mapping>

   <servlet-mapping>
      <servlet-name>Registrar</servlet-name>
      <url-pattern>/Registrar</url-pattern>
   </servlet-mapping>
</web-app>